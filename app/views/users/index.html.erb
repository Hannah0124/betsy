

<div class="users__index_container">
  <% @users.each do |user| %>
    <div class="users__index_user-container">
      <% if user.photo_url %>
        <%= image_tag user.photo_url, alt: 'user image', class: 'user_image' %>
      <% end %>
      <h6><%= link_to "#{user.name}", user_path(user), class: 'user_name' %></h6>
    </div>
  <% end %>
</div>

<div class="pages">
  <%= will_paginate @users, :style => 'color:white', class: 'user_name' %>
</div>